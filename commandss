sed = stream editor
        edit files
        replace data in files
        sed -n '1,3'p test.sh  =>> get 1 to 3 lines
        sed 's/<1>/<2>/g'  => find & replace 
        sed -i 's/<>/<>/g' filename  => interactive to update thefile

        awk {'print $1'} => print the first column
        awk /<string_to_search>/'{print}' fillename
        awk -F'[.]' '^version/'/{print}' filename
                -F delimmeter

